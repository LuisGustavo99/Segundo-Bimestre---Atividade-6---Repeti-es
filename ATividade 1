programa
{
   inclua biblioteca Matematica --> m
	
	funcao inicio()
	{
		inteiro pais_A = 80000
		inteiro pais_B = 200000
		inteiro anos = 0


		enquanto(pais_A < pais_B){ 

			pais_A = pais_A * 1.03
               pais_B = pais_B *  1.015
			anos ++

			
		}

		escreva("Se passaram ", anos, " anos até A alcançar B")
	}
}
